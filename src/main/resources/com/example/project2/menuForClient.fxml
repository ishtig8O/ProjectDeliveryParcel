<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1000.0" spacing="20.0" style="-fx-background-color: #989;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project2.Controller.MenuForClientController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label prefHeight="41.0" prefWidth="960.0" text="Меню для клиентов">
        <font>
            <Font size="72.0" />
        </font>
    </Label>

    <Label fx:id="welcomeText" />
    <AnchorPane prefHeight="553.0" prefWidth="960.0" style="-fx-background-color: #fafafa;">
      <children>
            <Button fx:id="buttonBack" layoutX="374.0" layoutY="343.0" mnemonicParsing="false" onAction="#switchToBack" prefHeight="26.0" prefWidth="109.0" text="Выйти" />
         <Button fx:id="buttonGoToCreateOrder" layoutX="179.0" layoutY="130.0" mnemonicParsing="false" onAction="#switchToBaseCreate" prefHeight="40.0" prefWidth="499.0" text="Создать заказ" />
         <Button fx:id="buttonGoToBaseDelivery" layoutX="177.0" layoutY="60.0" mnemonicParsing="false" onAction="#switchToBaseDelivery" prefHeight="40.0" prefWidth="499.0" text="Пункты доставки" />
         <Button fx:id="buttonToViewOrder" layoutX="177.0" layoutY="187.0" mnemonicParsing="false" onAction="#switchToRec" prefHeight="40.0" prefWidth="499.0" text="Посмотреть заказы" />
          <Button fx:id="buttonToChange" layoutX="177.0" layoutY="251.0" mnemonicParsing="false" onAction="#switchToChange" prefHeight="40.0" prefWidth="499.0" text="Изменить данные" />
      </children>
    </AnchorPane>
</VBox>
